{
  "name": "new-day",
  "version": "1.0.0",
  "description": "New day selling shop",
  "main": "app.js",
  "scripts": {
    "test": "better-npm-run test",
    "dev": "better-npm-run dev",
    "start": "yarn run migrate && yarn run seed && better-npm-run prod",
    "migrate": "npx sequelize-cli db:migrate",
    "undo-migrate": "npx sequelize-cli db:migrate:undo:all",
    "migrate-all": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "build": "babel src -d dist"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon --exec babel-node ./src/app.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "prod": {
      "command": "node ./dist/app.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "nyc mocha --require @babel/register ./__test__/ --recursive --timeout 2000 --exit",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "author": "Gerard",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "@types/i18next-fs-backend": "^1.1.2",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "better-npm-run": "^0.1.1",
    "bluebird": "^3.7.2",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.30.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "eslint-config-node": "^4.1.0",
    "express": "^4.17.3",
    "express-form-data": "^2.0.19",
    "heroku": "^7.33.3",
    "i18next": "^21.6.16",
    "i18next-fs-backend": "^1.1.4",
    "i18next-http-middleware": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.5",
    "nyc": "^15.1.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "pg-promise": "^10.11.1",
    "promisify": "^0.0.3",
    "sequelize": "^6.19.1",
    "sequelize-slugify": "^1.6.0",
    "swagger-ui-express": "^4.3.0",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "sequelize-cli": "^6.4.1"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
